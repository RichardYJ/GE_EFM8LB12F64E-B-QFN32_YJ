			//SMB0_I2C_MasterRead(FW_regAddr_base[0] + i);
			checkSum += mdioData;
			dataAddr += 1;	 // 2/1
			ramAddr += 2;
		}

#if MY_PRINTF_EN == 1
		printf("checksum =");
		tH =  checkSum>> 16;
		tL =  checkSum& 0xFFFF;
		printf("%04x", tH);
		printf("%04x", tL);
		printf("\r\n");
#endif
		SMB0_I2C_MasterWrite(FW_regAddr_base[0] + 14, (-checkSum) & 0xFFFF);
		SMB0_I2C_MasterRead(FW_regAddr_base[0] + 14);
		SMB0_I2C_MasterWrite(FW_regAddr_base[0] + 15, 0x800C);

		checktime = 0;
		status = SMB0_I2C_MasterRead(FW_regAddr_base[0] + 15);

		while (status == 0x800C) {
			checktime++;
			if (checktime > 1000)
#if MY_PRINTF_EN == 1
				printf("checktime =");
				tH =  checktime>> 16;
				tL =  checktime& 0xFFFF;
				printf("%04x", tH);
				printf("%04x", tL);
				printf("\r\n");
#endif
						;
			status = SMB0_I2C_MasterRead(FW_regAddr_base[0] + 15);
		}
		if (status != 0x0000) {
			TotalError++;
#if MY_PRINTF_EN == 1
			printf("checksum error!, total error	= ");
			tH =  TotalError>> 16;
			tL =  TotalError& 0xFFFF;
			printf("%04x", tH);
			printf("%04x", tL);
			printf("\r\n");
#endif
		}
	}	//end of the for loop

	SMB0_I2C_MasterWrite(FW_regAddr_base[0] + 12, enterPoint >> 16);
	SMB0_I2C_MasterWrite(FW_regAddr_base[0] + 13, enterPoint & 0xFFFF);
	checkSum = (enterPoint >> 16) + (enterPoint & 0xFFFF) + 0x4000;
	SMB0_I2C_MasterWrite(FW_regAddr_base[0] + 14, (-checkSum) & 0xFFFF);
	SMB0_I2C_MasterWrite(FW_regAddr_base[0] + 15, 0x4000);

	if (RdCheck) {
		checktime = 0;
		status = SMB0_I2C_MasterRead(FW_regAddr_base[0] + 15);
		while (status == 0x4000) {
			checktime++;
			if (checktime > 1000)
#if MY_PRINTF_EN == 1
				printf("checktime =");
				tH =  checktime>> 16;
				tL =  checktime& 0xFFFF;
				printf("%04x", tH);
				printf("%04x", tL);
				printf("\r\n");
#endif
						;
			status = SMB0_I2C_MasterRead(FW_regAddr_base[0] + 15);
		}
	}

	for (i = 0; i < 1024; i++) {
		for (x = 0; x < 1024; x++) {
			;
		}
	}
#if MY_PRINTF_EN == 1
	printf("check the result:\r\n");
#endif
	status = SMB0_I2C_MasterRead(0x9814);
#if MY_PRINTF_EN == 1
	printf("0x9814's  value:");
	tH =  status>> 16;
	tL =  status& 0xFFFF;
	printf("%04x", tH);
	printf("%04x", tL);

	printf("\r\n");
	printf("If there is checksum error, total error  = ");
	tH =  TotalError>> 16;
	tL =  TotalError& 0xFFFF;
	printf("%04x", tH);
	printf("%04x", tL);
	printf("\r\n");
#endif

	{	//load default:
#if MY_PRINTF_EN == 1
		printf("load default: ");
		printf("\r\n");
#endif
		SMB0_I2C_MasterWrite(0x9818, 0x5000);
		status = SMB0_I2C_MasterRead(0x9818);

		while (0x0500 != status) {
#if MY_PRINTF_EN == 1
