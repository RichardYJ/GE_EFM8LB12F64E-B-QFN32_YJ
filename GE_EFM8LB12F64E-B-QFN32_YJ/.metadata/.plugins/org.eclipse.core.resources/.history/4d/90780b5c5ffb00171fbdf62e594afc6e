	for (x = 0; x < section; x++) {
		checkSum = 0x800C;
		SMB0_I2C_MasterWrite(FW_regAddr_base[0] + 12, ramAddr >> 16);
		SMB0_I2C_MasterWrite(FW_regAddr_base[0] + 13, ramAddr & 0xFFFF);

		checkSum += (ramAddr >> 16) + (ramAddr & 0xFFFF);

		for (i = 0; i < 12; i++) // ramAddr 
				{
