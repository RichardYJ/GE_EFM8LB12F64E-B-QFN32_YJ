			EEPROM_Buffer[76] = rValue >> 8;
			EEPROM_Buffer[77] = rValue & 0x00ff;
			EEPROM_Buffer[78] = 0;    //complete
		}
#if 0		
		x++;
		if(x > 30000)	//history:	x>10 
