		}
	}

	{	//load 25g:
#if MY_PRINTF_EN == 1
		printf("load 25g: ");
		printf("\r\n");
#endif
		SMB0_I2C_MasterWrite(0x9818, 0x5020);
		status = SMB0_I2C_MasterRead(0x9818);
		while (0x0500 != status) {
#if MY_PRINTF_EN == 1
						printf("load default: 0x9818's	value:");
						tH =  status>> 16;
						tL =  status& 0xFFFF;
